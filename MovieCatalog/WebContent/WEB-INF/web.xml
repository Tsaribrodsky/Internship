<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
    id="WebApp_ID" version="3.1">
    <display-name>MovieCatalog</display-name>

    <servlet>
        <servlet-name>Display</servlet-name>
        <servlet-class>com.amdocs.catalog.Display</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Display</servlet-name>
        <url-pattern>/display/select</url-pattern>
        <url-pattern>/display/insert</url-pattern>
        <url-pattern>/display/delete</url-pattern>
        <url-pattern>/display/update</url-pattern>
    </servlet-mapping>

</web-app>